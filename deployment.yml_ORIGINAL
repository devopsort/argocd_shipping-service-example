apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    app: ms-shipping
  name: ms-shipping
  namespace: default 
spec:
  replicas: 1
  selector:
    matchLabels:
      app: ms-shipping
  template:
    metadata:
      labels:
        app: ms-shipping
    spec:
      containers:
        - image: #IMAGEN#
          name: shipping-service
          ports:
            - containerPort: 8080
              name: ms-shipping 
              protocol: TCP
          resources:
            limits:
              memory: "128Mi"
              cpu: "500m"
